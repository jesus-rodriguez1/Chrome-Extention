{
    "manifest_version": 2,

  "name": "Chrome Extension",
  "description": "This extension will open a tab and change the HtML/ CSS",
  "version": "1.0",
    
    "background": {
    "scripts": ["popup.js"]
  },
  "content_scripts": [
    {
      "matches": [
  "https://powerschool.noblenetwork.org/public/"
      ],
      "css": ["popup.css"],
      "js": ["jquery-3.2.1.min.js"]
    }
  ],
    
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
