{
    "manifest_version": 2,

  "name": "Chrome Extenion",
  "description": "This extension will open a tab and change the HtML/ CSS",
  "version": "1.0",
    "background": {
    "scripts": ["popup.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["popup.css"],
      "js": ["jquery-3.2.1.min.js", "change.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
